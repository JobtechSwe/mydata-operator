@html.extend('layout', function(model) {
  @html.block('content', function(model) {
    <h1>Settings</h1>
    <h2>Personal Data Store</h2>
    <p>Click on option below to setup your PDS provider</p>
    <ul class="mdl-list">
    @model.providers.forEach((provider) => (
      <li class="mdl-list__item">
        <a href="@provider.link"
          class="mdl-button mdl-button__expandable mdl-js-button mdl-button--raised mdl-button--accent">
          <img src="@provider.img" />
          @provider.name
        </a>
      </li>
    ))
      <li class="mdl-list__item">
          <button class="mdl-button mdl-button__expandable mdl-js-button mdl-button--raised mdl-button--accent" disabled style="width:150px;height:50px">
            Google Drive
          </button>
      </li>
      <li class="mdl-list__item">
          <button class="mdl-button mdl-button__expandable mdl-js-button mdl-button--raised mdl-button--accent" disabled style="width:150px;height:50px">
            FTP
          </button>
      </li>
      <li class="mdl-list__item">
          <button class="mdl-button mdl-button__expandable mdl-js-button mdl-button--raised mdl-button--accent" disabled style="width:150px;height:50px">
            Amazon S3
          </button>
      </li>
    </ul>
  })
})
